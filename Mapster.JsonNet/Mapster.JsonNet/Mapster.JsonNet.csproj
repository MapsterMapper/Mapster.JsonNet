<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net40;net45;netstandard1.3</TargetFrameworks>
    <Authors>Chaowlert Chaisrichalermpol</Authors>
    <Company>Chaowlert Chaisrichalermpol</Company>
    <Description>Json.net conversion supports for Mapster</Description>
    <PackageProjectUrl>https://github.com/MapsterMapper/Mapster.JsonNet</PackageProjectUrl>
    <RepositoryUrl>https://github.com/MapsterMapper/Mapster.JsonNet</RepositoryUrl>
    <PackageIconUrl>https://cloud.githubusercontent.com/assets/5763993/26522718/d16f3e42-4330-11e7-9b78-f8c7402624e7.png</PackageIconUrl>
    <PackageTags>Mapster;Json.net</PackageTags>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>Mapster.JsonNet.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Mapster" Version="3.1.5" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

</Project>
